                "./dist/recipe/index.d.ts"
            ],
            "recipe/session/framework/*": [
                "./dist/recipe/*"
            ],
            "recipe/session": [
                "./dist/recipe/session/index.d.ts"
            ],
            "recipe/session/claims": [
                "./dist/recipe/session/claims.d.ts"
            ],
            "recipe/thirdparty": [
                "./dist/recipe/thirdparty/index.d.ts"
            ],
            "recipe/thirdparty/providers/*": [
                "./dist/recipe/thirdparty/providers/*/index.d.ts"
            ],
            "recipe/thirdpartyemailpassword": [
                "./dist/recipe/thirdpartyemailpassword/index.d.ts"
            ],
            "recipe/thirdpartyemailpassword/emaildelivery": [
                "./dist/recipe/thirdpartyemailpassword/emaildelivery/index.d.ts"
            ],
            "recipe/thirdpartypasswordless": [
