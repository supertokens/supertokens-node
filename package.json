  },
  "scripts": {
    "test": "TEST_MODE=testing npx mocha --node-option no-experimental-fetch -r test/fetch-polyfill.mjs --timeout 500000",
    "test-exports": "./test/testExports.sh",
    "build-check": "cd lib && npx tsc -p tsconfig.json --noEmit && cd ../test/with-typescript && npm run build",
    "build-dts": "npx tsc -p tsconfig.json --emitDeclarationOnly",
    "build-files": "npx tsup --config tsup.config.ts",
    "build": "rm -rf lib/build && npm run build-files && npm run build-dts && cd ./test/with-typescript && npm run build && cd ../.. && npm run post-build",
    "pretty": "npx pretty-quick .",
    "post-build": "node add-ts-no-check.cjs",
    "build-pretty": "npm run build && npm run pretty",
    "pretty-check": "npx pretty-quick --check .",
    "set-up-hooks": "cp hooks/pre-commit.sh .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "build-docs": "rm -rf ./docs && npx typedoc --out ./docs --tsconfig ./tsconfig.json ./lib/ts/index.ts ./lib/ts/**/index.ts ./lib/ts/**/*/index.ts"
  },
  "keywords": [
    "auth",
    "authentication",
    "authorisation",
    "nodejs",
    "supertokens",
    "jwt",
    "refresh tokens"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supertokens/supertokens-node.git"
  },
